<div>
  <div>
    <div>

      <div>

      <div>
        <%= render 'profile', user: @users %>
      </div>

        <div>
          <div>
            <h4>投稿一覧</h4>
          </div>

          <% @post_images.each do |post_image| %>
          <div>
            <span>
              <%= link_to public_post_image_path(post_image.id) do %>
                <%= image_tag post_image.get_image(100,100) %>
              <% end %>
            </span>
            <span><%= link_to post_image.title, public_post_image_path(post_image.id) %></span>
            <span><%= post_image.created_at.strftime('%Y/%m/%d') %></span>
            <span><%= post_image.comments.count %></span>
            <span><%= post_image.bookmarks.count %></span>
          </div>
          <% end %>
        </div>

      </div>

    </div>
  </div>
</div>